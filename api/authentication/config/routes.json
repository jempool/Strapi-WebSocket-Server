{
  "routes": [
    {
      "method": "POST",
      "path": "/auth/login",
      "handler": "authentication.login"
    },
    {
      "method": "POST",
      "path": "/auth/signup",
      "handler": "authentication.signup"
    },
    {
      "method": "POST",
      "path": "/auth/refresh",
      "handler": "authentication.refreshToken"
    }
  ]
}
